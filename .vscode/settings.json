{
  "angular-template-formatter.indentWidth": 2,
  "css.validate": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/dist/*/**": true
  },
  "git.ignoreLimitWarning": true,
  "html.format.enable": false,
  "liveServer.settings.host": "angular-seo.localhost.tv",
  "liveServer.settings.AdvanceCustomBrowserCmdLine": "chrome --incognito --unsafely-treat-insecure-origin-as-secure=http://angular-seo.localhost.tv:5500 --allow-insecure-localhost http://angular-seo.localhost.tv:5500 --user-data-dir=c:/dev/test --ignore-certificate-errors",
  "liveServer.settings.root": "/dist/browser/",
  "prettier.disableLanguages": ["html", "json", "typescript"],
  "prettier.stylelintIntegration": true,
  "scss.validate": false,
  "search.exclude": {
    "**/coverage": true,
    "**/coverage_js": true,
    "**/dist": true
  },
  "tslint.autoFixOnSave": true,
  "tslint.rulesDirectory": "node_modules/codelyzer",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescriptHero.imports.grouping": ["Plains", "Modules", "/\\.\\./", "/\\./"],
  "typescriptHero.imports.organizeOnSave": true
}
