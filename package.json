{
  "name": "@fr-esco/angular-seo-seed",
  "version": "1.0.1",
  "description": "Example project for Angular 7 SEO-friendly webapp with SSR and Material.",
  "repository": "github:fr-esco/angular-seo-seed",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "run-p \"lint:ts --format stylish\" \"lint:css --formatter string\"",
    "lint:ts": "ng lint",
    "lint:css": "stylelint \"src/**/*.scss\" --cache --cache-location .stylelintcache",
    "e2e": "ng e2e",
    "ssr": "run-s build:ssr serve:ssr",
    "build:ssr": "run-s build:client-and-server-bundles webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run angular.io-example:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "keywords": [
    "angular",
    "seed",
    "universal",
    "ssr",
    "seo"
  ],
  "author": "Francesco Colamonici <f.colamonici@gmail.com> (https://francescocolamonici.app)",
  "license": "GPL-3.0",
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/pwa": "^0.11.4",
    "@angular/router": "^7.0.0",
    "@angular/service-worker": "^7.0.0",
    "@angular/upgrade": "^7.0.0",
    "@nguniversal/express-engine": "^7.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.0",
    "angular-in-memory-web-api": "^0.8.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.4",
    "@angular/cli": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/platform-server": "^7.0.0",
    "@fr-esco/tslint-config": "^2.0.2",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "^2.0.4",
    "@types/node": "~10.12.0",
    "codelyzer": "^4.5.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.2",
    "npm-run-all": "^4.1.3",
    "phantomjs-prebuilt": "^2.1.7",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "rxjs-tslint-rules": "^4.10.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.1",
    "ts-loader": "^5.2.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.0",
    "tslint-angular": "^1.1.2",
